{
    "name": "clizaola/simplecast",
    "description": "A Laravel Package wrapper around the Simplecast API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Lizaola",
            "email": "carlos@lizaola.net"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Clizaola\\Simplecast\\": "src"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clizaola\\Simplecast\\SimplecastServiceProvider"
            ],
            "aliases": {
                "Simplecast": "Clizaola\\Simplecast\\SimplecastFacade"
            }
        }
    }
}
